        "profile": "tabular-data-resource",
        "schema": {
            "fields": [{
                    "name": "DataColeta",
                    "type": "string",
                    "format": "default",
                    "title": "Data de Coleta no formato AAAA-MM-DD (ISO 8601)",
                    "description": "Data em que a amostra foi coletada no Posto Revendedor de Combustíveis.",
                    "constraints": {
                        "required": true,
                        "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}"
                    }
                },
                {
                    "name": "IdNumeric",
                    "type": "integer",
                    "format": "default",
                    "title": "Chave Primária",
                    "description": "Código Númerico que identifica uma amostra coletada.",
                    "constraints": {
                        "required": true
                    }
                },
                {
                    "name": "GrupoProduto",
                    "type": "string",
                    "format": "default",
                    "title": "Categoria do combustível",
                    "description": "Tipos de combustíveis: Etanol Gasolina e Óleo Diesel ",
                    "constraints": {
                        "required": true,
                        "enum": ["Gasolina", "Etanol", "Óleo Diesel"]
                    }
                },
                {
                    "name": "Produto",
                    "type": "string",
                    "format": "default",
                    "title": "Tipo de combustível",
                    "description": "Subdivisão de GrupoProduto.",
                    "constraints": {
                        "required": true
                    }
                },
                {
                    "name": "RazaoSocialPosto",
                    "type": "string",
                    "format": "default",
                    "description": "Razão social do Posto Revendedor de Combustível",
                    "title": "Razão Social",
                    "constraints": {
                        "required": true
                    }
                },
                {
                    "name": "CnpjPosto",
                    "type": "string",
                    "format": "default",
                    "title": "CNPJ",
                    "description": "O CNPJ do Posto Revendedor de Combustível.",
                    "constraints": {
                        "required": true,
                        "pattern": "[0-9]{2}\\.[0-9]{3}\\.[0-9]{3}/[0-9]{4}-[0-9]{2}"
                    }
                },
                {
                    "name": "Distribuidora",
                    "type": "string",
                    "format": "default",
                    "title": "Razão Social",
                    "description": "A razão social da distribuidora de combustível.",
                    "constraints": {
                        "required": true
                    }
                },
                {
                    "name": "Endereço",
                    "type": "string",
                    "format": "default",
                    "title": "Endereço",
                    "description": "Endereço do Posto Revendedor de Combustível."
                },
                {
                    "name": "Complemento",
                    "type": "string",
                    "format": "default",
                    "title": "Complemento",
                    "description": "Complemento do endereço do Posto Revendedor de Combustível."
                },
                {
                    "name": "Bairro",
                    "type": "string",
                    "format": "default",
                    "title": "Bairro",
                    "description": "Bairro do Posto Revendedor de Combustível."
                },
                {
                    "name": "Município",
                    "type": "string",
                    "format": "default",
                    "title": "Município",
                    "description": "Município onde está localizado o Posto Revendedor de Combustível."
                },
                {
                    "name": "Latitude",
                    "type": "number",
                    "format": "default",
                    "title": "Latitude",
                    "description": "Latitude do Posto Revendedor de Combustível"
                },
                {
                    "name": "Longitude",
                    "type": "number",
                    "format": "default",
                    "title": "Longitude",
                    "description": "Longitude do Posto Revendedor de Combustível"
                },
                {
                    "name": "Uf",
                    "type": "string",
                    "format": "default",
                    "title": "Unidade da Federação",
                    "description": "UF associada ao Posto Revendedor de Combustível.",
                    "constraints": {
                        "required": true,
                        "enum": [
                            "AC", "AL", "AM", "AP", "BA", "CE", "DF", "ES", "GO", "MA", "MG", "MS",
                            "MT", "PA", "PB", "PE", "PI", "PR", "RJ", "RN", "RO", "RR", "RS", "SC",
                            "SE", "SP", "TO"
                        ]
                    }
                },
                {
                    "name": "RegiaoPolitica",
                    "type": "string",
                    "format": "default",
                    "title": "Região Política",
                    "description": "Região Política do Posto Revendedor de Combustível.",
                    "constraints": {
                        "required": true,
                        "enum": ["CENTRO-OESTE", "NORDESTE", "NORTE", "SUDESTE", "SUL"]
                    }
                },
                {
                    "name": "Ensaio",
                    "type": "string",
                    "format": "default",
                    "title": "Ensaio físico-químico",
                    "description": "Nome do ensaio físico-químico realizada na amostra.",
                    "constraints": {
                        "required": true,
                        "enum": [
                            "Destilação - 10%", "Destilação - 50%", "Destilação - 90%", "Destilação - PFE",
                            "Destilação - Resíduo", "Destilação - 85%", "Destilação - 95%", "Teor de Enxofre",
                            "Massa Específica a 20°C", "Teor de Biodiesel", "Ponto de Fulgor", "Teor de Água e Sedimentos",
                            "Teor Alcoólico", "Teor de Hidrocarbonetos", "Teor de Etanol", "Contaminação total", "Aromáticos",
                            "Olefínicos", "Saturados", "Material Particulado", "Material Não Volátil", "Condutividade Elétrica",
                            "Cor", "Potencial Hidrogeniônico (pH)", "Aspecto", "IAD", "MON", "RON", "Teor de Água", "Cor ASTM",
                            "Água Livre", "Teor de Marcador", "Teor de Metanol", "Destilação - Perda", "Benzeno",
                            "Número de Cetano Derivado"
                        ]

                    }
                },
                {
                    "name": "Resultado",
                    "type": "any",
                    "format": "default",
                    "title": "Resultado",
                    "description": "Resultado do ensaio físico-químico",
                    "constraints": {
                        "required": true
                    }
                },
                {
                    "name": "UnidadeEnsaio",
                    "type": "string",
                    "format": "default",
                    "title": "Unidade",
                    "description": "Unidade do ensaio físico-químico"
                },
                {
                    "name": "Conforme",
                    "type": "string",
                    "format": "default",
                    "title": "Avaliação de Conformidade",
                    "description": "Avalia se o resultado é conforme ou não conforme a especificação vigente.",
                    "constraints": {
                        "required": true,
                        "enum": ["Sim", "Não"]
                    }
                }
            ]
        },
        "encoding": "utf8",
        "title": "PMQC",
        "description": "\n"
    }],
    "keywords": [
        "anp",
        "combustivel",
        "gasolina",
        "diesel",
        "etanol",
        "qualidade"
